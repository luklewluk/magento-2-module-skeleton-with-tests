{
  "name": "luklewluk/magento-2-module-skeleton-with-tests",
  "description": "Magento 2 Module skeleton with tests",
  "require": {
    "php": "~7.0.0",
    "magento/framework": "100.*.*",
    "magento/module-store" : "100.*.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.3",
    "symfony/console": "~2.3"
  },
  "type": "magento2-module",
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  },
  "autoload": {
    "files": [
      "src/registration.php"
    ],
    "psr-4": {
      "luklew\\ModuleSkeleton\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "luklew\\ModuleSkeleton\\Tests\\": "tests/"
    }
  }
}
