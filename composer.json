{
  "name": "luklewluk/magento-2-module-skeleton-with-tests",
  "description": "Magento 2 Module skeleton with tests",
  "require": {
    "magento/framework": "^100.0|^101.0",
    "magento/module-store": "^100.0|^101.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.3",
    "symfony/console": "~2.3"
  },
  "type": "magento2-module",
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Luklewluk\\ModuleSkeleton\\": ""
    }
  }
}
